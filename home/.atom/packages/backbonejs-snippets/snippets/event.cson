      ${1:object}.stopListening({1:[other]}, {2:[event]}, {3:[callback]});
  """
  "Event.listenTo":
    "prefix": "ev.listento"
    "body": """
      ${1:object}.listenTo({1:other}, {2:event}, {3:callback});
    """
  "Event.listenToOnce":
    "prefix": "ev.once"
    "body": """
      ${1:object}.once({1:other}, {2:event}, {3:callback});
    """
  "Event.listenToOnce":
    "prefix": "ev.listenonce"
    "body": """
      ${1:object}.listenToOnce({1:other}, {2:event}, {3:callback});
    """
  "Event.trigger":
    "prefix": "ev.trigger"
    "body": """
      ${1:object}.trigger({1:event}, {2:[@args]});
    """
  "Event.off":
    "prefix": "ev.off"
    "body": """
      ${1:object}.off({1:[event]}, {2:[callback]}, {3:[context]});
    """
  "Event.on":
    "prefix": "ev.on"
    "body": """
      ${1:object}.on({1:[event]}, {2:[callback]}, {3:[context]});
    """
